{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 71,
      "minLength": 3
    },
    "slug": {
      "type": "uid",
      "required": true,
      "minLength": 3,
      "maxLength": 110
    },
    "description": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "size": {
      "type": "string",
      "maxLength": 70
    },
    "color": {
      "type": "enumeration",
      "enum": [
        "red",
        "green",
        "blue"
      ],
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "availableQty": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "carts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::cart.cart",
      "mappedBy": "product"
    },
    "categoryname": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    }
  }
}
